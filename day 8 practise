
#2
class  Account:

    def __init__(self,bal,acc):
        self.balance = bal
        self.account_no = acc

    def debit(self , amount):
        self.balance -= amount
        print("rs" , self.balance , "is debited")
        print("total balance is" , self.get_balance())

    def credit(self , amount):
        self.balance += amount
        print("rs", self.balance ,"is credited")
        print("total balance is" , self.get_balance())

    def get_balance(self):
        return self.balance



acc1 = Account(1000,2000)
acc1.debit(1000)
acc1.credit(2000)

#3
class Student:
    college_name = "Ramaiah institute of technology"
    print(college_name)
    name = "saksham" #class attr
    def __init__(self , name , marks):
        self.name = name #obj attr (obj attr > class attr)
        self.marks = marks
        print("students")

        #methods or functions
    def welcome(self):
        print("welcome student")

    def get_marks(self):
        return(self.marks)    

s1 = Student("karan" , 90)
print(s1.name , s1.marks)

s2 = Student("arjun" , 95)
print(s2.name , s2.marks)

s1.welcome()
print(s1.get_marks())

        
